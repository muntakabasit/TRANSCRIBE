modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dawt-transcribe-server"

[[workflows.workflow]]
name = "dawt-transcribe-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn main:app --host 0.0.0.0 --port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 37531
externalPort = 3000

[[ports]]
localPort = 40313
externalPort = 3001

[nix]
channel = "stable-25_05"
packages = ["ffmpeg", "ffmpeg-full", "libxcrypt", "pkg-config"]

[deployment]
deploymentTarget = "autoscale"
run = ["uvicorn", "main:app", "--host", "0.0.0.0", "--port", "5000"]
