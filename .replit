modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dawt-transcribe-server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "design-scraper"

[[workflows.workflow]]
name = "dawt-transcribe-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn main:app --host 0.0.0.0 --port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "design-scraper"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd design-scraper && uvicorn app:app --host 0.0.0.0 --port 6000"
waitForPort = 6000

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 6000
externalPort = 6000

[[ports]]
localPort = 37531
externalPort = 3000

[[ports]]
localPort = 40313
externalPort = 3001

[[ports]]
localPort = 46129
externalPort = 3002

[nix]
channel = "stable-25_05"
packages = ["ffmpeg", "ffmpeg-full", "freetype", "gitFull", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "pkg-config", "playwright-driver", "tcl", "tk", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["uvicorn", "main:app", "--host", "0.0.0.0", "--port", "5000"]
